{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true
  },
  "extends": ["plugin:vue/recommended"],
  "plugins": [
    "vue",
    "babel"
  ],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "vue/no-v-html": "off",
    "vue/no-parsing-error": [
      "error",
      {
        "x-invalid-end-tag": false
      }
    ]
  }
}
